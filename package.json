{
  "name": "jakal-node",
  "version": "1.1.0",
  "description": "no description",
  "main": "./dist/server/src/index.js",
  "scripts": {
    "_build:app": "BABEL_ENV=browser webpack --config ./lib/app/webpack.config.babel.js",
    "_build:server": "BABEL_ENV=server babel ./lib --out-dir ./dist --soure-map --copy-files",
    "_do:triggerschedule": "node -e 'require(\"./dist/server/src/scheduled\").actUponPeriodChange()'",
    "test:app": "echo \"All good for now...\" && exit 0 #TOOL",
    "test:server": "BABEL_ENV=server NODE_ENV=test mocha --require ./lib/server/mocha.setup.js ./lib/server/src/**/*.test.js",
    "dev:build": "NODE_ENV=development npm run _build:app -- -w & NODE_ENV=development npm run _build:server -- -w",
    "dev:start": "NODE_ENV=development nodemon ./dist/server/src/index.js",
    "dev:triggerschedule": "NODE_ENV=development npm run _do:triggerschedule",
    "prebuild": "/bin/rm -rf ./dist",
    "build": "NODE_ENV=production npm run _build:app -- -p && NODE_ENV=production npm run _build:server",
    "start": "echo 'Start with you favourite [node|pm2|...]' $npm_package_main",
    "triggerschedule": "NODE_ENV=production npm run _do:triggerschedule"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monkeybeans/jakal-node.git"
